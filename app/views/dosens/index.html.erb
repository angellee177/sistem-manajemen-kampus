<p id="notice"><%= notice %></p>

<h1>Dosens</h1>

<table>
  <thead>
    <tr>
      <th>Nama</th>
      <th>Tanggal lahir</th>
      <th>Alamat</th>
      <th>Pendidikan terakhir</th>
      <th>No telp</th>
      <th>Jabatan</th>
      <th>Picture</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dosens.each do |dosen| %>
      <tr>
        <td><%= dosen.nama %></td>
        <td><%= dosen.tanggal_lahir %></td>
        <td><%= dosen.alamat %></td>
        <td><%= dosen.pendidikan_terakhir %></td>
        <td><%= dosen.no_telp %></td>
        <td><%= dosen.jabatan %></td>
        <td><%= image_tag dosen.picture_url, width: '200px' if dosen.picture.present? %></td>
        <td><%= link_to 'Show', dosen %>|</td>
        <td><%= link_to 'Edit', edit_dosen_path(dosen) %>|</td>
        <td><%= link_to 'Destroy', dosen, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dosen', new_dosen_path %>

